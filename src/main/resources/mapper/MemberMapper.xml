<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.deepen.mapper.MemberMapper">

	
	<select id="getUserById" parameterType="String" resultType="com.deepen.domain.MemberDTO">
		select *
		from members 
		where id = #{id}
	</select>
	
	
</mapper>



